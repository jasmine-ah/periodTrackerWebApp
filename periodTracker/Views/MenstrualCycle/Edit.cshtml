@model MenstrualCycle

@{
    ViewData["Title"] = "Edit Menstrual Cycle";
    Layout = "~/Views/Shared/_Layout_login.cshtml";
}

<h2>@ViewData["Title"]</h2>

<form asp-action="Edit">
    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <label asp-for="StartDate"></label>
        <input asp-for="StartDate" class="form-control" />
        <span asp-validation-for="StartDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="EndDate"></label>
        <input asp-for="EndDate" class="form-control" />
        <span asp-validation-for="EndDate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="CycleLength"></label>
        <input asp-for="CycleLength" class="form-control" />
        <span asp-validation-for="CycleLength" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="PeriodLength"></label>
        <input asp-for="PeriodLength" class="form-control" />
        <span asp-validation-for="PeriodLength" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save Changes</button>
</form>

